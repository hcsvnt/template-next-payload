{
    "name": "template-next-payload",
    "version": "1.0.0",
    "description": "A template for creating a Next.js app with Payload CMS",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
        "build:analyze": "cross-env NODE_OPTIONS=--no-deprecation ANALYZE_BUNDLE=true next build",
        "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
        "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
        "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
        "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
        "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
        "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
        "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
        "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
    },
    "dependencies": {
        "@payloadcms/db-mongodb": "latest",
        "@payloadcms/next": "latest",
        "@payloadcms/payload-cloud": "latest",
        "@payloadcms/richtext-lexical": "latest",
        "@vanilla-extract/css": "^1.16.1",
        "cross-env": "^7.0.3",
        "effect": "^3.10.19",
        "framer-motion": "^11.12.0",
        "graphql": "^16.8.1",
        "next": "^15.0.3",
        "payload": "latest",
        "react": "19.0.0-rc-69d4b800-20241021",
        "react-dom": "19.0.0-rc-69d4b800-20241021",
        "sharp": "0.33.5",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^15.0.0",
        "@types/node": "^22.10.1",
        "@types/react": "^19.0.0-rc.1",
        "@types/react-dom": "^19.0.0-rc.1",
        "@typescript-eslint/eslint-plugin": "^8.15.0",
        "@typescript-eslint/parser": "^8.15.0",
        "@vanilla-extract/next-plugin": "^2.4.7",
        "eslint": "^9.15.0",
        "eslint-config-next": "15.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-functional": "^7.1.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-perfectionist": "^4.0.3",
        "eslint-plugin-promise": "^7.2.0",
        "prettier": "^3.3.3",
        "typescript": "5.7.2",
        "typescript-eslint": "^8.16.0"
    },
    "engines": {
        "node": "^18.20.2 || >=20.9.0"
    },
    "pnpm": {
        "overrides": {
            "@types/react": "npm:types-react@19.0.0-rc.1",
            "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
        }
    },
    "overrides": {
        "@types/react": "npm:types-react@19.0.0-rc.1",
        "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
}
